{
  "source": {
    "include": ["backend/src"],
    "includePattern": ".js$",
    "exclusePattern": "backend/src/__tests__",
    "exclude": ["backend/src/__tests__"]
  },
  "plugins": ["plugins/markdown"],
  "templpates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "./jsdoc-template"
  }
}